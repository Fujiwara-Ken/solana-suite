  },
  "homepage": "https://github.com/atonoy/solana-suite#readme",
  "dependencies": {
    "form-data": "^4.0.0",
    "nft.storage": "^3.3.0",
    "node-fetch": "^3.0.0",
    "@solana-suite/shared": "../shared"
  }
