{
  "name": "solana-suite",
  "main": "dist/cjs/src/index.js",
  "module": "dist/esm/src/index.js",
  "types": "dist/cjs/src/index.d.ts",
  "version": "1.0.0",
  "engines": {
    "node": ">=14.*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "network:devt": "sed -i -e 's/\"network\":.*\"/\"network\": \"dev\"/' tsconfig.json",
    "network:mainnet": "sed -i -e 's/\"network\":.*\"/\"network\": \"mainnet-beta\"/' tsconfig.json",
    "test": "mocha -t 300000 --require ts-node/register -- ",
    "test:all": "mocha -t 300000 --require ts-node/register 'test/**/*.test.ts'"
  },
  "dependencies": {
    "form-data": "^4.0.0",
    "nft.storage": "^3.3.0",
    "node-fetch": "^3.0.0"
  }
}
