{
  "name": "solana-suite",
  "main": "dist/cjs/src/index.js",
  "module": "dist/esm/src/index.js",
  "types": "dist/cjs/src/index.d.ts",
  "version": "0.0.1",
  "engines": {
    "node": ">=14.*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "network:devt": "sed -i -e 's/\"network\":.*\"/\"network\": \"dev\"/' tsconfig.json",
    "network:mainnet": "sed -i -e 's/\"network\":.*\"/\"network\": \"mainnet-beta\"/' tsconfig.json",
    "test": "mocha -t 300000 --require ts-node/register -- ",
    "test:all": "mocha -t 300000 --require ts-node/register 'test/**/*.test.ts'",
    "lint": "npx tslint --fix --project ."
  },
  "workspaces": [
    "packages/"
  ],
  "dependencies": {
    "form-data": "^4.0.0",
    "nft.storage": "^3.3.0",
    "node-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@solana/buffer-layout": "^3.0.0",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.29.2",
    "@tsconfig/node14": "^1.0.1",
    "@tsconfig/recommended": "^1.0.1",
    "@types/bs58": "^4.0.1",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^8.2.3",
    "@types/node": "^14.17.5",
    "typescript": "^4.4.3",
    "chai": "^4.3.4",
    "mocha": "^8.4.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^9.1.1",
    "tslib": "^1.14.1",
    "tslint": "^6.1.3"
  }
}
